{
  "name": "chrt-object",
  "version": "0.0.0",
  "description": "Chart Object components of Chrt",
  "main": "dist/chrt-object.js",
  "module": "dist/chrt-object.esm.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "test": "jest",
    "pretest": "rollup -c",
    "postversion": "rollup -c",
    "build": "rollup -c",
    "lint": "eslint src --fix",
    "watch": "npm-watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/chrt/chrt-object.git"
  },
  "author": "chrt",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/chrt/chrt-object/issues"
  },
  "homepage": "chrt.io",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.20.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "npm-watch": "^0.7.0",
    "rollup": "2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "watch": "^1.0.2"
  },
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ]
}
